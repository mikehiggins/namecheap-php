{
    "name": "wirecore/namecheap-php",
    "type": "library",
	"version": "1.2.0",
    "description": "PHP Package for Namecheap API",
    "keywords": [
        "wirecore",
        "namecheap-php"
    ],
    "homepage": "https://github.com/vendor/package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":Florian Laabs",
            "email": "florian.laabs@wirecore.de",
            "homepage": "http://www.wirecore.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Wirecore\\Namecheap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
